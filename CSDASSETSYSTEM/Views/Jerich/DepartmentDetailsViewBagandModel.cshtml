@model List<CSDASSETSYSTEM.Models.Core.Department>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <title>DepartmentDetailsViewBagandModel</title>
</head>
<body>
    <div>
        @ViewBag.WelcomeMessage

        <table>
            <tr>
                <th>Id</th>
                <th>Department Code</th>
                <th>Department Name</th>
            </tr>
            @if (Model != null)
            {
                foreach (var dept in Model)
                {
                    <tr>
                        <td>@dept.Id<td>
                        <td>@dept.DepartmentCode</td>
                        <td>@dept.DepartmentName</td>
                    </tr>
                }
            }

        </table>
    </div>
</body>
</html>
