
@{
    ViewBag.Title = "Home Page";
}




<table class="table table-bordered table-hover table-striped">
    <tr>
        <th>
            Id
        </th>
        <th>
            CompanyId
        </th>
        <th>
            Name
        </th>
        <th>
            Department
        </th>
        <th>
            Actions
        </th>
    </tr>

    @if (ViewBag.ListOfPerson != null)
    {
        foreach (var item in (ViewBag.ListOfPerson as List<CSDASSETSYSTEM.Models.Core.Person>))
        {
                @Html.Partial("PersonListDetailPartialView",item)
        }
    }



</table>


